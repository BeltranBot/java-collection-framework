package com.cf03.HashMap;

import java.util.HashMap;

public class App {
	
	public static void main(String[] args) {
		HashMap<Integer, String> map = new HashMap<Integer, String>();
		
		map.put(5, "five");
		map.put(8, "eight");
		map.put(6, "six");
		map.put(4, "four");
		map.put(2, "two");
		
		String text = map.get(1);
		System.out.println(text);
		
		
		
	}

}
